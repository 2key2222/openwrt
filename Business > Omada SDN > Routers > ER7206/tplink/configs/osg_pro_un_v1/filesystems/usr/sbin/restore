. /lib/functions.sh

#tftp server ip:192.168.1.100,GE5 ip:192.168.1.1
cd /tmp/
tftp -gr user_config.tar 192.168.1.100

mv /tmp/user_config.tar /etc/
rm -rf /etc/config/

cd /etc/
tar -xf user_config.tar
rm -rf user_config.tar