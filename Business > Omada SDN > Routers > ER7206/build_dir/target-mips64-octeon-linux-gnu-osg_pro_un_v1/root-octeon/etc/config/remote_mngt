config support
	list service HTTP
	list service HTTPS
	list service TELNET
	list service SSH

config reject
	option enabled 'off'
	list service SSH

config service port
	option HTTP "80"
	option TELNET "23"
	option SSH "22"
	option HTTPS "443"

config mngt_iface 
	option iface 'wan1_eth'				
	option enabled 'off'				

config mngt_iface
	option iface 'wan1_poe'				
	option enabled 'off'				

