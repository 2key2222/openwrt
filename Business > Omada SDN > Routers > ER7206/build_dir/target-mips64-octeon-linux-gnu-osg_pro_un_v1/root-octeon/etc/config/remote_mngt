	option iface 'wan4_eth'				
	option enabled 'off'				

config mngt_iface
	option iface 'wan4_poe'				
	option enabled 'off'				

config mngt_iface
	option iface 'lan'				
	option enabled 'on'

config lan_rsa_rule 'NETWORK1'
	option enabled 'on'
	option ipaddr '192.168.0.0/24'
