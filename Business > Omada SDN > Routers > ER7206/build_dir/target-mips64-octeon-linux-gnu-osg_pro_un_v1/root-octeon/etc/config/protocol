
config interface 'wan1_v6'
    option enable 'off'
    option proto 'none'

config interface 'wan2_v6'
    option enable 'off'
    option proto 'none'

config interface 'wan3_v6'
    option enable 'off'
    option proto 'none'

config interface 'wan4_v6'
    option enable 'off'
    option proto 'none'

config interface 'lanv6'
    option enable 'off'

config proto 'staticv6'
    option auto '1'
    option mtu '1500'
    option wan_type 'staticv6'
    option proto 'static'

config proto 'dhcpv6'
    option auto '1'
    option lanif 'br-lan'
    option peerdns '1'
    option ip_config 'dhcpv6'
    option ip_mode 'non_temp'
    option wan_type 'dhcp6c'
    option dns_mode 'dynamic'
    option proto 'dhcp6c'

config proto 'pppoev6'
    option auto '1'
    option lanif 'br-lan'
    option dns_mode 'dynamic'
    option mru '1492'
    option ip_mode 'non_temp'
    option ip_config 'dhcpv6'
    option wan_type 'pppoev6'
    option proto 'pppoev6'
    option keeplive '5 20'

config proto 'pppoeshare'
    option auto '1'
    option lanif 'br-lan'
    option mru '1492'
    option wan_type 'pppoe'
    option proto 'pppoeshare'
    option keeplive '5 20'
    option conn_mode 'auto'
    option enable 'on'
    option connectable '1'

config proto '6to4'
    option auto '1'
    option dns_mode 'dynamic'
    option wan_type '6to4'
    option ttl '255'
    option proto '6to4'

config proto '6rd'
    option ip_mode 'auto'
    option auto '1'
    option wan_type '6rd'
    option ttl '255'
    option proto '6rd'

config proto 'dslite'
    option auto '1'
    option wan_type 'dslite'
    option proto 'dslite'
    option snd_conn 'dynamic'
    option dynamic_ip_config 'dhcpv6'
    option dynamic_ip_mode 'non_temp'
    option dynamic_dns_mode 'dynamic'
    option pppoe_dns_mode 'dynamic'
    option pppoe_ip_mode 'non_temp'
    option pppoe_ip_config 'dhcpv6'
    option keeplive '5 20'
    option lanif 'br-lan'
    option ttl '255'
    option mtu '1492'
    option static_mtu '1500'

config proto 'passthrough'
    option auto '1'
    option wan_type 'passthrough'
    option ttl '255'
    option proto 'passthrough'

config proto 'dhcp6s'
    option auto '1'
    option prefixlen '64'
    option leasetime '86400'
    option endip '2000'
    option startip '1000'
    option proto 'dhcp6s'
    option ifname 'br-lan'

config proto 'slaac'
    option auto '1'
    option prefixlen '64'
    option proto 'slaac'
    option ifname 'br-lan'

config proto 'rdnss'
    option auto '1'
    option rdnss_prefix_type 'static'
    option prefixlen '64'
    option proto 'rdnss'
    option ifname 'br-lan'

config connproto 'ipv6'
    list protos 'staticv6'
    list protos 'dhcp6c'
    list protos 'pppoev6'
    list protos '6to4'
    list protos '6rd'
    list protos 'dslite'
    list protos 'passthrough'
