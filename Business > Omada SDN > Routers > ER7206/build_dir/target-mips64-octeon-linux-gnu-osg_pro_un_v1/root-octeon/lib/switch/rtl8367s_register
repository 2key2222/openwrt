RTL8367C_STORM_FILTER_IDX_MASK=0xff
RTL8367C_STORM_FILTER_BROAD_IDX_BASE=0x0a44
RTL8367C_STORM_FILTER_MULTI_IDX_BASE=0x0a4c

RTL8367C_SHARE_METER_CTRL_BASE=0x1400

RTL8367C_SHARE_METER_IFG_CTRL=0x1712
#portvlan
RTL8367C_REG_VLAN_MEMBER_CONFIGURATION0_CTRL0=0x0728
RTL8367C_REG_TABLE_ACCESS_ADDR=0x0501

