config wifi-device 'wifi0'
	option type 'qcawifi'
	option phy 'wifi0'
	option country 'CN'

	option wds_sta 'ath00'
	option basic_ap 'ath01'

	option hwmode 'bgn'
	option htmode 'auto'
	option channel '0'
	option txpower '32'

	option maxsta '50'
	option isolate 'off'

	option rts '2346'
	option frag '2346'
	option dtim_period '1'
	option wmm 'on'
	option shortgi 'on'
	option bintval '100'

	option radio_enable 'on'
	option mssid_enable 'off'
	option mac_key_mark '0'
	option radio_id '0'
	option band '2g'
	
config wifi-iface 'ath00'
	option ifname 'ath00'
	option device 'wifi0'
	option hidden 'off'
	option ssid ''
	option bssid ''
	
	option encryption 'none'
	option psk_version 'auto'
	option psk_cipher 'auto'
	option wpa_version 'auto'
	option wpa_cipher 'auto'
	option wep_mode 'open'
	option curPassword ''
	option wep_key1 ''
	option wep_type1 ''
	option wep_key2 ''
	option wep_type2 ''
	option wep_key3 ''
	option wep_type3 ''
	option wep_key4 ''
	option wep_type4 ''
	
	option chanbw '0'
	option txqueuelen '1000'
	option htweptkip '0'
	option mcastenhance '2'
	option doth '1'
	option ignore11d '1'
	option disablecoext '1'
	option shortgi '1'
	option extap '0'
	option vap_ind '1'
	option mode 'sta'
	
	option wep_format1 '16'
	option wep_format2 '16'
	option wep_format3 '16'
	option wep_format4 '16'
	option wep_select '1'
	
	option psk_key ''
	option channel_wds ''
	
	option wds '0'
	option enable 'off'

config wifi-iface 'ath01'
	option ifname 'ath01'
	option device 'wifi0'
	option mode 'ap'
	option enable 'on'

	option ssid 'TP-LINK_2.4G'
	option describe ''
	option encryption 'none'
	option wep_mode 'auto'
	option wep_select '0'
	option wep_format '0'
	option wep_type1 '0'
	option wep_key1 ''
	option wep_type2 '0'
	option wep_key2 ''
	option wep_type3 '0'
	option wep_key3 ''
	option wep_type4 '0'
	option wep_key4 ''

	option psk_version 'auto'
	option psk_cipher 'auto'
	option psk_group_rekey '86400'
	option psk_key ''

	option wpa_version 'auto'
	option wpa_cipher 'auto'
	option wpa_group_rekey '86400'
	option server ''
	option port ''
	option wpa_key ''

	option guest 'off'
	option hidden 'off'
	option isolate 'off'

	option key_idx '1'
	option ap_idx '1'

	option macfilter 'off'
	option rule 'deny'
	option wds '1'
	option athnewind '1'
	
config wifi-device 'wifi1'
	option type 'qcawifi'
	option phy 'wifi1'
	option country 'CN'

	option wds_sta 'ath10'
	option basic_ap 'ath11'

	option hwmode 'anac'
	option htmode 'auto'
	option channel '0'
	option txpower '32'

	option maxsta '50'
	option isolate 'off'

	option rts '2346'
	option frag '2346'
	option dtim_period '1'
	option wmm 'on'
	option shortgi 'on'
	option bintval '100'

	option radio_enable 'on'
	option mssid_enable 'off'
	option mac_key_mark '0'
	option radio_id '1'
	option band '5g'

