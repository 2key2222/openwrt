config wifi-iface 'ath11'
	option ifname 'ath11'
	option device 'wifi1'
	option mode 'ap'
	option enable 'on'

	option ssid 'TP-LINK_5G'
	option describe ''
	option encryption 'none'
	option wep_mode 'auto'
	option wep_select '0'
	option wep_format '0'
