config interface_mode
     option wanmode '1'
    option switch_enable '1'
	 option singlewan '0'
    option wan_ifname 'agl0.10|agl0.20|agl0.30|agl0.40'
    option lan_ifname 'eth0|eth0|eth0|eth0'

config interface 'wan1_eth'
    option t_type 'physical'
    option t_name 'wan1_eth'
    option t_issys '1'
    option t_reference '0'
    option proto 'dhcp'
    option ifname 'agl0.10'
    option macaddr '02:12:AB:B7:2A:00'
	
config interface 'wan1_poe'
	option ifname 'agl0.10'
	option t_bindif 'agl0'
	option metric '0'
	option t_name 'wan1_poe'
	option lcpechofailure '5'
	option proto 'pppoe'
	option auto '0'
	option peerdns '1'
	option t_linktype 'auto'
	option t_reference '0'
	option mtu '1492'
	option t_type 'pppoe'
	option lcpechointerval '2'
	option t_issys '0'
	option t_poe_enable '0'
	

