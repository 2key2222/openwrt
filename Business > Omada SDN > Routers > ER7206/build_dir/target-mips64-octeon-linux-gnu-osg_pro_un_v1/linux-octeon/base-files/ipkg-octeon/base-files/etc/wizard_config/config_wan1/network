config interface 'lan'
    option t_type 'physical'
    option t_name 'lan'
    option t_issys '1'
    option t_reference '0'
    option proto 'static'
    option ifname 'eth0'
    option ipaddr '192.168.1.1'
    option netmask '255.255.255.0'
    option macaddr '02:12:AB:B7:2A:04'

config interface 'loopback'
    option ipaddr '127.0.0.1'
    option proto 'static'
    option netmask '255.0.0.0'
    option ifname 'lo'

config status_interface

config t_has_switch       
    option wan_vlan_id '10|20|30|40'
    option lan_ports_value '6 1 2 3 4|6 2 3 4|6 3 4|6 4'
    option wan_ports_value '7t 0|7t 1|7t 2|7t 3'
config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option ports '6 1 2 3 4'

config switch_vlan
	option device 'switch0'
	option vlan '10'
	option ports '7t 0'

