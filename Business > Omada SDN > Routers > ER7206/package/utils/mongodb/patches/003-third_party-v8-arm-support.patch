+    virtual LInstruction* instr() { return instr_; }
+   private:
+    LNumberTagI* instr_;
+  };
+
+  Register src = ToRegister(instr->InputAt(0));
+  Register dst = ToRegister(instr->result());
+
+  DeferredNumberTagI* deferred = new(zone()) DeferredNumberTagI(this, instr);
