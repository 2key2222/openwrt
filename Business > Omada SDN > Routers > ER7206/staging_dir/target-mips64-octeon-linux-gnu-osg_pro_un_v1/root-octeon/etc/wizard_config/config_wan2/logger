config logger setting
        option refresh        'on'
		option level_switch   'off'
		option level          'ALL'
		option log_send       'off'
		option send_server    '0.0.0.0'
        option module_switch  'off'
        option module         'ALL'
		option enable         'on'
		option serverip       '0.0.0.0'

