From a1568c35f88be852f026c4e1aea26af8690b7ccd Mon Sep 17 00:00:00 2001
From: Igal Liberman <igall@marvell.com>
Date: Sun, 30 Oct 2016 14:22:23 +0200
Subject: [PATCH 1034/2241] dts: cp110: change sata compatible to
 marvell,armada-cp110-ahci

Change-Id: I73bfac4d0e45949f0e39425bc4a094bd130078d0
Signed-off-by: Igal Liberman <igall@marvell.com>
Reviewed-on: http://vgitil04.il.marvell.com:8080/33469
Reviewed-by: Wilson Ding <dingwei@marvell.com>
Reviewed-by: Omri Itach <omrii@marvell.com>
Tested-by: Omri Itach <omrii@marvell.com>
---
 arch/arm64/boot/dts/marvell/armada-cp110-1.dtsi | 2 +-
 arch/arm64/boot/dts/marvell/armada-cp110.dtsi   | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/marvell/armada-cp110-1.dtsi b/arch/arm64/boot/dts/marvell/armada-cp110-1.dtsi
index 77fd1d2..bf506c9 100644
--- a/arch/arm64/boot/dts/marvell/armada-cp110-1.dtsi
+++ b/arch/arm64/boot/dts/marvell/armada-cp110-1.dtsi
@@ -48,7 +48,7 @@ pinctrl@440000 {
 };
 
 sata@540000 {
-	compatible = "marvell,armada-3700-ahci";
+	compatible = "marvell,armada-cp110-ahci";
 	reg = <0x540000 0x30000>;
 	dma-coherent;
 	interrupts = <GIC_SPI 287 IRQ_TYPE_LEVEL_HIGH>;
diff --git a/arch/arm64/boot/dts/marvell/armada-cp110.dtsi b/arch/arm64/boot/dts/marvell/armada-cp110.dtsi
index 8d8de5e..47d7f7e 100644
--- a/arch/arm64/boot/dts/marvell/armada-cp110.dtsi
+++ b/arch/arm64/boot/dts/marvell/armada-cp110.dtsi
@@ -118,7 +118,7 @@ serial@702300 {
 };
 
 sata: sata@540000 {
-	compatible = "marvell,armada-3700-ahci";
+	compatible = "marvell,armada-cp110-ahci";
 	reg = <0x540000 0x30000>;
 	dma-coherent;
 	interrupts = <GIC_SPI 63 IRQ_TYPE_LEVEL_HIGH>;
