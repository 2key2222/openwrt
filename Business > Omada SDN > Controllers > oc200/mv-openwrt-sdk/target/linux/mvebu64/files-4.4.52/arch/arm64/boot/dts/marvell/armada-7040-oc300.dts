/*  * Device Tree file for OC300 board */

#include "armada-7040-db.dtsi"

/ {
	model = "Marvell Armada 7040 oc300 board";
	compatible = "marvell,armada7040-db-A", "marvell,armada7040-db", "marvell,armada7040",
			"marvell,armada-ap806-quad", "marvell,armada-ap806", "marvell,armada-7040-oc300";
	#address-cells = <2>;
	#size-cells = <2>;

	chosen { };

	ap806 {
		config-space {
			serial@512000 {
				status = "okay";
			};
			sdhci@6e0000 {
				status = "disabled";
			};
			i2c@511000 {
				status = "disabled";
			};

			spi0: spi@510600 {
				status = "okay";
				spi-flash@0 {
					#address-cells = <0x1>;
					#size-cells = <0x0>;
					compatible = "jedec,spi-nor", "marvell,armada-380-spi";
					spi-max-frequency = <20000000>;
					reg = <0x510600 0x50>;
					cell-index = <0>;
					interrupts = <GIC_SPI 21 IRQ_TYPE_LEVEL_HIGH>;
					clocks = <&syscon 3>;
				};
			};
		};
	};

	cpn-110-master {
		config-space {
			pinctrl@440000 {
				system_led_pins: system-led-pins {
					marvell,pins = "mpp30";
					marvell,function = "gpio";
				};
				system_reset_pins: system-reset-pins {
					marvell,pins = "mpp12";
					marvell,function = "gpio";
				};
				system_usb_pins: system-usb-pins {
					marvell,pins = "mpp49";
					marvell,function = "gpio";
				};
			};
			sata: sata@540000 {
				status = "disabled";
				sata-port@0 {
					status = "disabled";
				};
				sata-port@1 {
					status = "disabled";
				};
			};
			spi@700680 {
				status = "disabled";
			};
			
			serial@702100 {
				status = "okay";
			};
			serial@702200 {
				status = "okay";
			};

			usb3h1: usb3@510000 {
				phys = <&comphy0 3 COMPHY_USB3H1>; 
				phy-names = "comphy";
				status = "okay";
			};

			axim-hb1-rd@3c8000 {
				status = "disabled";
			};
			axim-hb1-wr@3c9000 {
				status = "disabled";
			};

			sdhci@780000 {
				/*vqmmc-supply = <&vccq_sdio_reg>;*/
 				compatible = "marvell,armada-cp110-sdhci";
 				reg = <0x780000 0x300>;
 				interrupts = <0x0 27 4>;
 				clock-names = "core";
 				clocks = <&cpm_syscon0 1 4>;
 				dma-coherent;
 				marvell,xenon-phy-tun-step-divider = <25>;
				bus-width = <4>;
				non-removable;
				pinctrl-names = "default";
				pinctrl-0 = <&cpm_sdhci_pins>;
				status = "okay";
			};
			mdio@12a200 {
				#address-cells = <1>;
				#size-cells = <0>;
				mdio1_phy1: ethernet-phy@1 {
					reg = <0>;
				};
			};
			gop {
				emac0: mac0 {
					phy-mode = "none"; /* lane-2 */
					phys = <&comphy0 2 COMPHY_SFI>;
					phy-names = "comphy";
				};
				emac2: mac2 {
					phy-mode = "rgmii"; /* disabled */
					fixed-link {
						speed = <1000>;
						full-duplex;
					};
				};
				emac3: mac3 {
					phy-mode = "none"; /* disabled */
				};
			};
			ppv22@000000 {
				l4_chksum_jumbo_port = <1>;
				eth0: eth0@010000 {
					status = "disabled";
				};
				eth1: eth1@020000 {
					status = "okay";
				};
				eth2: eth2@030000 {
					status = "disabled";
				};
			};
			eip197: eip197@800000 {
				status = "okay";
			};
			comphy {
				status = "okay";
			};
			

		};
		pcie@0x640000 {
			status = "disabled";
		};
		pcie@0x620000 {
			status = "disabled";
		};

		gpio-leds {
			compatible = "gpio-leds";
			pinctrl-0 = <&system_led_pins>;
			pinctrl-names = "default";

			system-led {
				label = "system-led";
				gpios = <&cpm_gpio0 30 GPIO_ACTIVE_HIGH>;
				default-state = "off";
			};
		};

		gpio-keys {
			compatible = "gpio-keys";
			pinctrl-0 = <&system_reset_pins>;
			pinctrl-names = "default";

			reset-button {
				label = "reset";
				linux,code = <0x198>;
				gpios = <&cpm_gpio0 12 GPIO_ACTIVE_LOW>;
			};
		};
	};
};
