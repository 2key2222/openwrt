From 00441ed12d4dbc01413b6762481de7564eb858d6 Mon Sep 17 00:00:00 2001
From: Zhoujie Wu <zjwu@marvell.com>
Date: Tue, 22 Aug 2017 16:26:59 -0700
Subject: [PATCH 1974/2241] arm64: dts: marvell: armada3720: enable SD power
 sequence

Change-Id: Ib6163f597c0d89056322b4bd396226fab68cfab0
Signed-off-by: Zhoujie Wu <zjwu@marvell.com>
Reviewed-on: http://vgitil04.il.marvell.com:8080/43544
Tested-by: iSoC Platform CI <ykjenk@marvell.com>
Reviewed-by: Kostya Porotchkin <kostap@marvell.com>
Reviewed-by: Victor Gu <xigu@marvell.com>
Reviewed-by: Hanna Hawa <hannah@marvell.com>
---
 arch/arm64/boot/dts/marvell/armada-3720-db.dts | 6 ++++++
 1 file changed, 6 insertions(+)

diff --git a/arch/arm64/boot/dts/marvell/armada-3720-db.dts b/arch/arm64/boot/dts/marvell/armada-3720-db.dts
index 7ce8868..12c523af 100644
--- a/arch/arm64/boot/dts/marvell/armada-3720-db.dts
+++ b/arch/arm64/boot/dts/marvell/armada-3720-db.dts
@@ -200,6 +200,7 @@
 				bus-width = <4>;
 				cd-gpios = <&gpiosb 2 GPIO_ACTIVE_LOW>;
 				vqmmc-supply = <&vcc_sd_reg1>;
+				mmc-pwrseq = <&sd_pwrseq>;
 				marvell,pad-type = "sd";
 				status = "okay";
 
@@ -228,6 +229,11 @@
 				status = "okay";
 			};
 
+			sd_pwrseq: sd-pwrseq {
+				compatible = "mmc-pwrseq-a3700-sd";
+				pwren-gpios = <&expander0 4 GPIO_ACTIVE_HIGH>;
+			};
+
 			vcc_sd_reg1: regulator@0 {
 				compatible = "regulator-gpio";
 				regulator-name = "vcc_sd1";
-- 
2.7.4

