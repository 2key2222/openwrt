From 61d40ffe9e55159658fadddd96242b8d2e83e648 Mon Sep 17 00:00:00 2001
From: Hanna Hawa <hannah@marvell.com>
Date: Thu, 27 Oct 2016 11:23:57 +0300
Subject: [PATCH 1041/2241] mmc: cp110: add SDIO node for CP-110

Change-Id: I5cff72dc82684d0df207185a326dd188da00f626
Signed-off-by: Hanna Hawa <hannah@marvell.com>
Reviewed-on: http://vgitil04.il.marvell.com:8080/33386
Reviewed-by: Omri Itach <omrii@marvell.com>
Tested-by: Omri Itach <omrii@marvell.com>
---
 arch/arm64/boot/dts/marvell/armada-cp110.dtsi | 11 +++++++++++
 1 file changed, 11 insertions(+)

diff --git a/arch/arm64/boot/dts/marvell/armada-cp110.dtsi b/arch/arm64/boot/dts/marvell/armada-cp110.dtsi
index 47d7f7e..f54330c 100644
--- a/arch/arm64/boot/dts/marvell/armada-cp110.dtsi
+++ b/arch/arm64/boot/dts/marvell/armada-cp110.dtsi
@@ -269,6 +269,17 @@ mdio@15b000 {
 	reg = <0x12a600 0x10>;
 };
 
+sdhci@780000 {
+	compatible = "marvell,xenon-sdhci";
+	reg = <0x780000 0x300>;
+	interrupts = <GIC_SPI 69 IRQ_TYPE_LEVEL_HIGH>;
+	clock-names = "core";
+	clocks = <&cpm_syscon0 1 4>;
+	xenon,slotno = <0>;
+	dma-coherent;
+	status = "disabled";
+};
+
 axim-cp-rd@3c5000 {
 	compatible = "marvell,coresight-axim", "arm,primecell";
 	reg = <0x3c5000 0x1000>;
